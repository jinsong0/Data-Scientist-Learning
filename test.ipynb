   "source": [
    "import numpy as np\n",
    "\n",
    "# 创建一个一维数组\n",
    "arr = np.array([1, 2, 3, 4, 5])\n",
    "\n",
    "# 对数组中的每个元素进行平方操作\n",
    "squared = np.square(arr)\n",
    "\n",
    "# 计算数组的平均值\n",
    "mean_value = np.mean(arr)\n",
    "\n",
    "# 输出结果\n",
    "print(\"原始数组:\", arr)\n",
    "print(\"平方后的数组:\", squared)\n",
    "print(\"数组的平均值:\", mean_value)"
   ],
